{
  "name": "dev_portal",
  "version": "2.0.0",
  "private": true,
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint . --fix",
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "scripts": {
    "dev": "next dev --port 3000",
    "build": "next build",
    "start": "next start",
    "pre-commit": "lint-staged",
    "pre-push": "tsc --project tsconfig.json",
    "postbuild": "next-sitemap --config next-sitemap.config.js",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "eslint": "npx eslint .",
    "eslint-fix": "npx eslint . --fix",
    "configure-husky": "npx husky install && npx husky add .husky/pre-commit \"npx --no-install lint-staged\"",
    "analyze": "cross-env ANALYZE=true next build",
    "generate": "graphql-codegen --config ./data/gql/codegen.ts",
    "knip": "knip",
    "prettier": "prettier --write .",
    "test": "npm run build && npm run test:unit && npm run test:e2e",
    "test:unit": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "clean": "npx shx rm -rf node_modules .next .turbo coverage dist compiled build-next-static build-storybook-static && npx shx rm -rf node_modules && npx shx echo Done!"
  },
